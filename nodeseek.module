#!name=NodeSeek Daily Check-in
#!desc=NodeSeek每日签到。登录账号进入设置页面以获取Token。
#!author=Sliverkiss (original), wellsgz (Egern port)
#!homepage=https://github.com/Sliverkiss
#!icon=https://raw.githubusercontent.com/sooyaaabo/Loon/main/Icon/App-Icon/NodeSeek.png

[Script]
nodeseek_cookie = type=http-response, pattern=^https:\/\/www\.nodeseek\.com\/api\/account\/getInfo\/.+\?readme=1&signature=1&phone=1, script-path=https://gist.githubusercontent.com/Sliverkiss/2b5acc2c4960dd06618c6912302c2c7f/raw/nodeseek.js, requires-body=true, timeout=60

nodeseek_checkin = type=cron, cronexp="0 10 * * *", script-path=https://gist.githubusercontent.com/Sliverkiss/2b5acc2c4960dd06618c6912302c2c7f/raw/nodeseek.js, timeout=60, wake-system=1

[MITM]
hostname = %APPEND% www.nodeseek.com
